{
    "name": "darthsoup/laravel-whmcs",
    "description": "Laravel WHMCS API interface",
    "license": "MIT",
    "authors": [
        {
            "name": "Kevin Krummnacker",
            "email": "kevin.krummnacker@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "illuminate/support": "^5.2|^6|^7|^8",
        "illuminate/contracts": "^5.2|^6|^7|^8",
        "guzzlehttp/guzzle": "~6.0|~7.0"
    },
    "require-dev": {
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~5.0|~6.0|~7.0|~8.0",
        "orchestra/testbench": "3.x"
    },
    "autoload": {
        "psr-4": {
            "DarthSoup\\Whmcs\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DarthSoup\\Tests\\Whmcs\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DarthSoup\\Whmcs\\WhmcsServiceProvider"
            ],
            "aliases": {
                "Whmcs": "DarthSoup\\Whmcs\\Facades\\Whmcs"
            }
        }
    }
}
